    <script type="text/javascript">
        jQuery(document).ready(function() {
            $("tr.order_q").click(function() {
                if ($(this).next('tr').css("display") != "none") {
                    $(this).next('tr').hide();
                    $(this).removeClass("current");
                } else {
                    $("tr.order_a").css('display', 'current');
                    $("tr.order_q").removeClass("current");
                    $(this).next('tr').show();
                    $(this).addClass("none");
                }
            });
        });
