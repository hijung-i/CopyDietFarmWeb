        }
        @media screen and (min-width:1080px){
        .like {top: 3px !important;right:5px !important;}
        /*(like-no { background: url("/images/beforeheart_ico_main.png") no-repeat;}*/
        .like-yes { background: url("/images/afterheart_icon_main.png") no-repeat}
        }
    @media screen and (max-width:1079px) {

       /* Modal Content/Box */
       .web_product_details {display: none}
        .sidebar{display:none}
        .review_modal, .inquiry_modal{
            position: fixed; /* Stay in place */
            z-index: 1; /* Sit on top */
            left: 0;
            top: 0;
            width: 100%; /* Full width */
            height: 100%; /* Full height */
            overflow: auto; /* Enable scroll if needed */
            background-color: rgb(0,0,0); /* Fallback color */
            background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
        }
        /* Modal Content/Box */
        .review_modal .modal-content {
            background-color: #fefefe;
            margin: 15% auto; /* 15% from the top and centered */
            width:400px!important;
            padding:10px;
            z-index:99999;
                            
        }

        .inquiry_modal .modal-content {
            background-color: #fefefe;
            margin: 15% auto; /* 15% from the top and centered */
            width:400px!important;
            padding:10px;
            height:500px;
            z-index:99999                       
        }
        /* The Close Button */
        .modal-content .close {
            color: #aaa;
            float: right;
            font-size: 28px;

        }
        .modal-content .close:hover,
        .modal-content .close:focus {
            color: black;
            text-decoration: none;
            cursor: pointer;
        }
    }
    @media screen and (min-width:1080px) {
        #content{max-width:1080px;margin:0 auto;width:1080px}
        .slick-prev{top:230px!important}
        .slick-next{top:-300px!important}
        .modal_wrap, .modal_wrap02 {
            width: 500px;
            height: auto;
            position: absolute;
            top:40%;
            left: 50%;
            margin: -250px 0 0 -250px;
            background:#eee;
            z-index: 2;
        }
        .black_bg, .black_bg02{
            position: absolute;
            content: "";
            width: 100%;
            height: 100%;
            background-color:rgba(0, 0,0, 0.5);
            top:0;
            left: 0;
            z-index: 1;
        }
        .modal_close, .modal_close02{
            width: 26px;
            height: 26px;
            position: absolute;
            top: -30px;
            right: 0;
        }
        .modal_close> a, .modal_close02 a{
            display: block;
            width: 100%;
            height: 100%;
            background:url(/images/x_icon_login.png)no-repeat;
            text-indent: -9999px;
            position:relative;
            top:42px
        }
        .mobile_product_details { display: none}                       
    }
