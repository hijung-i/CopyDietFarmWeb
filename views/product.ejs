        function Show_hidden(e) {
            var menu = new Array("test_1", "test_2", "test_3", "test_4", "test_5"); // 객체 배열로 지정
            for (var i = 0; i < menu.length; i++) {
                if ("test_" + e == menu[i]) {
                    document.all[menu[i]].style.display = "block";
                } else {
                    document.all[menu[i]].style.display = "none";
                }
            }
        }
